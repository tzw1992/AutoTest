[
  {
    "description": "这是一个带cookies信息的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "这是一个需要携带cookie信息才能访问的get请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "这是一个带cookie信息才能访问的Post请求",
    "request":{
      "uri": " /post/with/cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "zhangsan",
        "age": "20"
      }
    },
    "response": {
      "status": 202,
      "json": {
        "zhangsan": "success",
        "status": "1"
      }
    }

  }
]